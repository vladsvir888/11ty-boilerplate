<section class="contacts-block section">
  <div class="container contacts-block__container">
    {% include "../../@atoms/title/title.twig" with {
      title: {
        class: "contacts-block__title contacts-block__title--1",
        level: 2,
        text: "Головной офис"
      }
    } %}

    <div id="map_with_placemark" class="map contacts-block__map" data-coordinates="55.654371, 38.049914"></div>

    <div class="contacts-block__info">
      <div class="contacts-block__info-title">Контактные данные ООО «Ритейл Логистик»</div>
      <dl class="contacts-block__properties">
        <div class="contacts-block__property">
          <dt class="contacts-block__key">
            Юридический адрес
          </dt>
          <dd class="contacts-block__value">
            МО, Раменский р-н, п. Родники, ул. Трудовая, д.10 (склад №3), лит. 1Б
          </dd>
        </div>
        <div class="contacts-block__property">
          <dt class="contacts-block__key">
            ИНН
          </dt>
          <dd class="contacts-block__value">
            5040114204
          </dd>
        </div>
        <div class="contacts-block__property">
          <dt class="contacts-block__key">
            ОГРН
          </dt>
          <dd class="contacts-block__value">
            1125040002097
          </dd>
        </div>
        <div class="contacts-block__property">
          <dt class="contacts-block__key">
            Телефон
          </dt>
          <dd class="contacts-block__value">
            {% include "../../@atoms/button/button.twig" with {
              button: {
                class: "btn--link",
                href: "tel:+74957888860",
                text: {
                  text: "+7 (495) 788-88-60"
                }
              }
            } %}
          </dd>
        </div>
        <div class="contacts-block__property">
          <dt class="contacts-block__key">
            E-mail
          </dt>
          <dd class="contacts-block__value">
            {% include "../../@atoms/button/button.twig" with {
              button: {
                class: "btn--link",
                href: "mailto:info@vintegra.ru",
                text: {
                  text: "info@vintegra.ru"
                }
              }
            } %}
          </dd>
        </div>
        <div class="contacts-block__property">
          <dt class="contacts-block__key">
            Офис
          </dt>
          <dd class="contacts-block__value">
            Москва Сормовский проезд, д.11/2
          </dd>
        </div>
      </dl>
    </div>

    {% include "../../@atoms/title/title.twig" with {
      title: {
        class: "contacts-block__title contacts-block__title--2",
        level: 2,
        text: "Пункты выдачи заказов"
      }
    } %}

    <ul class="contacts-block__list">
      {% for item in items %}
        <li>
          {% include "../../^molecules/sales_rep_card/sales_rep_card.twig" with {
            sales_rep_card: item
          } %}
        </li>
      {% endfor %}
    </ul>

    {% include "../../@atoms/button/button.twig" with {
      button: {
        class: "btn--btn btn--tertiary contacts-block__more",
        text: {
          text: "Показать еще"
        }
      }
    } %}

    {% include "../../^molecules/pagination/pagination.twig" with {
      pagination: {
        class: "contacts-block__pagination",
        items: [{
          href: "",
          text: {
            text: "1"
          },
          current_page: true
        }, {
          href: "#",
          text: {
            text: "2"
          }
        }, {
