          button: {
            class: "btn--btn btn--outline personal-account__filter-btn js-drawer-btn",
            text: {
              text: personal_account.type == 'selected_filters' ? "Фильтр (3)" : "Фильтр"
            },
            icon: {
              name: "sprite-mono.svg#filter"
            },
            attrs: "type='button'"
          }
        } %}
      </form>

