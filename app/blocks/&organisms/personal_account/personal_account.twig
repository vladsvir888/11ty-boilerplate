      <div class="personal-account__btns-block toggle-group">
        {% include "../../@atoms/button/button.twig" with {
          button: {
            class: "toggle-group__btn",
            href: "#",
            text: {
              text: "Мой профиль"
            },
            attrs: personal_account.btns.btns_block_1.btn_1.attrs
          }
        } %}

        {% include "../../@atoms/button/button.twig" with {
          button: {
            class: "toggle-group__btn",
            href: "#",
            text: {
              text: "Мои заказы"
            },
            attrs: personal_account.btns.btns_block_1.btn_2.attrs
