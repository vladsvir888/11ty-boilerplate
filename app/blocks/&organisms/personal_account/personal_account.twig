      <form class="personal-account__filter-block" action="/" method="get" autocomplete="off">
        {% include "../../@atoms/form_control/form_control.twig" with {
          form_control: {
            view: "select",
            select: {
              class: "personal-account__select",
              label: "Сортировать по:",
              initial_value: "option-1",
              options: [{
                value: "option-1",
                text: "Самые последние вверху"
              }, {
                value: "option-2",
                text: "Самые последние снизу"
              }]
            }
          }
        } %}

        <noscript class="personal-account__no-js-form">
          <form class="no-js-form" action="/" method="get">
            <div class="select-wrapper">
              <select class="select select--no-js" name="sort">
                <option value="option-1">Самые последние вверху</option>
                <option value="option-2">Самые последние снизу</option>
              </select>
            </div>
            {% include "../../@atoms/button/button.twig" with {
              button: {
                class: "btn--link no-js-form__btn",
                text: {
                  text: "Отсортировать"
                }
              }
            } %}
          </form>
        </noscript>

        {% include "../../&organisms/search_box/search_box.twig" with {
          search_box: {
            class: "search-box--secondary search-box--tertiary",
            num_product_result: "Найдено 0 товаров",
            placeholder: "Введите номер заказа",
            num_product_result_mobile: "Найдено 0 заказов"
          }
        } %}

